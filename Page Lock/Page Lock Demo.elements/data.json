            "content" : "@portal(pageStart)\n<?php\nsession_start();\n\n$pagePassword = 'changeme';\n$errorMessage = null;\n\nif (isset($_GET['logout'])) {\n    unset($_SESSION['authenticated']);\n    header('Location: ' . strtok($_SERVER['REQUEST_URI'], '?'));\n    exit;\n}\n\nif (!empty($_POST['password'])) {\n    if (hash_equals($pagePassword, $_POST['password'])) {\n        $_SESSION['authenticated'] = true;\n    } else {\n        $errorMessage = 'Incorrect password. Please try again.';\n    }\n}\n\n$isAuthenticated = !empty($_SESSION['authenticated']);\n?>\n@endportal\n\n    <?php if (!$isAuthenticated): ?>\n        <div class=\"w-full max-w-sm\">\n            <div class=\"bg-slate-900\/60 backdrop-blur border border-slate-700 rounded-xl shadow-lg p-8\">\n                <h1 class=\"text-xl font-semibold mb-2 text-white\">Enter Password<\/h1>\n                <p class=\"text-sm text-slate-300 mb-6\">This page is locked. Please enter the password to continue.<\/p>\n                <?php if ($errorMessage): ?>\n                    <div class=\"mb-4 rounded-lg bg-red-500\/20 border border-red-500\/50 text-red-200 px-3 py-2 text-sm\">\n                        <?= htmlspecialchars($errorMessage, ENT_QUOTES, 'UTF-8') ?>\n                    <\/div>\n                <?php endif; ?>\n                <form method=\"post\" class=\"space-y-4\" x-data=\"{ isProcessing: false }\" x-on:submit=\"isProcessing = true\">\n                    <div>\n                        <label for=\"password\" class=\"block text-sm font-medium text-slate-200 mb-1\">Password<\/label>\n                        <input\n                            type=\"password\"\n                            name=\"password\"\n                            id=\"password\"\n                            required\n                            class=\"w-full rounded-lg border border-slate-700 bg-slate-900\/70 px-3 py-2 text-slate-100 placeholder:text-slate-400 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500 transition\"\n                            placeholder=\"••••••••\"\n                        >\n                    <\/div>\n                    <button\n                        type=\"submit\"\n                        class=\"inline-flex w-full items-center justify-center rounded-lg bg-blue-500 hover:bg-blue-400 active:bg-blue-600 px-4 py-2 font-medium text-white transition disabled:opacity-60 disabled:cursor-not-allowed\"\n                        x-bind:disabled=\"isProcessing\"\n                    >\n                        <span x-show=\"!isProcessing\">Unlock<\/span>\n                        <span x-cloak x-show=\"isProcessing\" class=\"flex items-center gap-2\">\n                            <svg class=\"h-4 w-4 animate-spin\" viewBox=\"0 0 24 24\" fill=\"none\">\n                                <circle class=\"opacity-25\" cx=\"12\" cy=\"12\" r=\"10\" stroke=\"currentColor\" stroke-width=\"4\"><\/circle>\n                                <path class=\"opacity-75\" fill=\"currentColor\" d=\"M4 12a8 8 0 018-8v4a4 4 0 00-4 4H4z\"><\/path>\n                            <\/svg>\n                            Processing...\n                        <\/span>\n                    <\/button>\n                <\/form>\n            <\/div>\n        <\/div>\n    <?php else: ?>\n        <div class=\"w-full max-w-2xl\">\n            <div class=\"bg-slate-900\/60 backdrop-blur border border-slate-700 rounded-xl shadow-lg p-10 space-y-6\">\n                <div class=\"flex items-center justify-between\">\n                    <div>\n                        <p class=\"text-sm uppercase tracking-wide text-blue-400\">Access Granted<\/p>\n                        <h1 class=\"text-2xl font-semibold text-white mt-1\">Welcome to the protected content<\/h1>\n                    <\/div>\n                    <a href=\"?logout=1\" class=\"inline-flex items-center rounded-lg border border-slate-700 px-3 py-2 text-sm text-slate-300 hover:bg-slate-800 transition\">\n                