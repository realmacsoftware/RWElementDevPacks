                }
            },
            {
                "title": "Via",
                "id": "secondColor",
                "format": "via-{{value}}",
                "themeColor": {
                    "default": {
                        "name": "purple",
                        "brightness": 500
                    }
                }
            },
            {
                "title": "To",
                "id": "thirdColor",
                "format": "to-{{value}}",
                "themeColor": {
                    "default": {
                        "name": "pink",
                        "brightness": 500
                    }
                }
            },
            {
                "information": {},
                "title": "Track Details"
            },
            {
                "title": "Background",
                "id": "trackColor",
                "format": "bg-{{value}}",
                "themeColor": {
                    "default": {
                        "name": "surface",
                        "brightness": 100
                    }
                }
            },
            {
                "title": "Opacity",
                "id": "opacity",
                "format": "bg-opacity-[{{value}}%]",
                "slider": {
                    "default": 50,
                    "min": 0,
                    "max": 100,
                    "units": "%",
                    "round": true
                }
