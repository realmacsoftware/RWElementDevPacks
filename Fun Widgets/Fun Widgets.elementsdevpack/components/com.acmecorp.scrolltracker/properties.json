                        "bottomRight": "none",
                        "bottomLeft": "none"
                      }
                    }
                  }
                },
                {
                    "title": "Shadow",
                    "id": "boxShadow",
                    "themeShadow": {
                        "default": {
                            "name": "DEFAULT"
                        }
                    }
                },
            ]
        },
        {
        "title": "Colour",
        "icon": "paintpalette.fill",
        "properties": [
            {
                "title": "From",
                "id": "firstColor",
                "format": "from-{{value}}",
                "themeColor": {
                    "default": {
                        "name": "blue",
